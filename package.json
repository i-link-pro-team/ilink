{
  "name": "ilink",
  "description": "Console ilink tools",
  "version": "0.0.4",
  "license": "MIT",
  "author": "EndyKaufman <admin@site15.ru>",
  "keywords": [
    "consul",
    "env",
    "sync",
    "files-to-consul",
    "files2consul",
    "cli",
    "ilink"
  ],
  "bugs": {
    "url": "https://github.com/i-link-pro-team/ilink/issues"
  },
  "homepage": "https://github.com/i-link-pro-team/ilink",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-link-pro-team/ilink.git"
  },
  "maintainers": [
    {
      "name": "EndyKaufman",
      "email": "admin@site15.ru"
    }
  ],
  "readme": "README.md",
  "scripts": {
    "nx": "nx",
    "rucken": "rucken",
    "start": "node ./dist/libs/ilink/src/main.js",
    "build": "npm run nx -- build ilink && ./scripts/add-header-to-ilink.js && npm run lib:build-changelog",
    "release": "standard-version -a",
    "publish": "npm publish ./dist/libs/ilink",
    "create-release": "npm run test && npm run build && npm run release && git push --follow-tags origin develop",
    "test": "npm run build && npm run nx -- test ilink --skip-nx-cache --json --outputFile=jest.result.json --testLocationInResults",
    "lint": "npm run tsc:lint && nx workspace-lint && nx run-many --target=lint --all",
    "lint:fix": "npm run tsc:lint && nx workspace-lint --fix && nx run-many --target=lint --all --fix && nx format:write --all",
    "tsc:lint": "tsc --noEmit -p tsconfig.base.json",
    "lib:build-changelog": "./node_modules/.bin/cp-cli ./CHANGELOG.md ./dist/libs/ilink/CHANGELOG.md",
    "generate": "npm run rucken -- prepare --locales=en,ru --default-locale=en && npm run lint:fix",
    "create-readme": "npm run start -- env-to-consul --help",
    "prepare": "husky install"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/cli": "14.6.5",
    "@nrwl/eslint-plugin-nx": "14.6.5",
    "@nrwl/jest": "14.6.5",
    "@nrwl/js": "14.6.5",
    "@nrwl/linter": "14.6.5",
    "@nrwl/node": "14.6.5",
    "@nrwl/workspace": "14.6.5",
    "@types/consul": "^0.40.0",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.1",
    "@types/pg": "8.6.5",
    "@types/recursive-readdir": "2.2.1",
    "@types/replace-ext": "2.0.0",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "cp-cli": "2.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "log4js": "6.6.1",
    "nestjs-console": "8.0.0",
    "nx": "14.6.5",
    "prettier": "2.7.1",
    "recursive-readdir": "2.2.2",
    "replace-ext": "2.0.0",
    "rucken": "^4.2.1",
    "sort-paths": "1.1.1",
    "standard-version": "9.5.0",
    "testcontainers": "8.13.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "consul": "^1.0.1",
    "dotenv": "^16.0.2",
    "fast-deep-equal": "3.1.3",
    "lodash.mergewith": "4.6.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.1.0",
    "source-map-support": "0.5.21",
    "tslib": "2.0.0"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "npm run generate && npm run lib:build-changelog && git add ."
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,css,scss,md}": "prettier --ignore-unknown --write"
  }
}
